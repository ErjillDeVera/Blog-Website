<%- include("partials/header"); -%>
  <h1>Compose</h1>
  <form action="/compose" method="post">
    <div class="form-group">
      <label>Title</label>
      <input class="form-control" type="text" name="blogTitle">

      <label>Post</label>
      <textarea name="form-control" rows="8" cols="164" name="blogPost"></textarea>
